{
  "manifest_version": 3,
  "name": "AI Browsing Insight - 您的年度浏览总结",
  "version": "1.0",
  "description": "Utilize AI to summarize your monthly and yearly browsing history.",
  "permissions": [
    "history",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "AI Browsing Insight"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "dashboard.html"
}